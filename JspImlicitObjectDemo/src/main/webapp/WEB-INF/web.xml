<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <description></description>
    <display-name>ImplicitObjectJsp</display-name>
    <servlet-name>ImplicitObjectJsp</servlet-name>
    <servlet-class>com.ciq.controller.ImplicitObjectJsp</servlet-class>
    <init-param>
    <param-name>user</param-name>
    <param-value>root</param-value>
    </init-param>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>ImplicitObjectJsp</servlet-name>
    <url-pattern>/demo</url-pattern>
  </servlet-mapping>
  <context-param>
  <param-name>context</param-name>
  <param-value>applicationScopeContext</param-value>
  </context-param>
  <error-page>
  <error-code>404</error-code>
  <location>/page.jsp</location>
  </error-page>
</web-app>